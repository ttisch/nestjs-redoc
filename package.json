{
  "name": "@juicyllama/nestjs-redoc",
  "version": "2.3.10",
  "description": "NestJS ReDoc Frontend For Express",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttisch/nestjs-redoc"
  },
  "bugs": "https://github.com/ttisch/nestjs-redoc",
  "author": {
    "name": "Thomas Tisch",
    "email": "t.tisch@live.com",
    "url": "https://github.com/ttisch/nestjs-redoc"
  },
  "main": "dist/index.js",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:clear": "npx jest --clearCache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "release-it --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "joi": "^17.7.0",
    "express-basic-auth": "^1.2.1",
    "express-handlebars": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.4.1",
    "@nestjs/core": "^9.3.1",
    "@nestjs/platform-express": "^9.3.1",
    "@nestjs/testing": "^9.3.1",
    "@types/express": "^4.17.16",
    "@types/express-handlebars": "^6.0.0",
    "@types/hapi__joi": "^17.1.9",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^29.4.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "yarn prepare",
        "yarn build"
      ],
      "after:release": "echo âœ… Successfully released ${name} v${version} to ${npm.name}."
    },
    "npm": {
      "publish": true,
      "timeout": 100000
    },
    "github": {
      "release": true,
      "timeout": 100000
    },
    "git": {
      "commit": true,
      "tag": true,
      "push": true,
      "requireCleanWorkingDir": false
    }
  }
}
